<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.embed.mapper.UsMapper">
	<insert id="insertUs" parameterType="org.embed.dto.UsDTO">
 <![CDATA[
 	INSERT INTO us
 	(
 	user_name,
 	user_password,
 	name,
 	phone,
 	email,
 	birth,
 	gender
 	)
 	VALUES
 	(
 	#{userName},
 	#{userPassword},
 	#{name},
 	#{phone},
 	#{email},
 	#{birth},
 	#{gender}
 	)
 
 ]]>
	</insert>
	<select  parameterType="string" resultType="org.embed.dto.UsDTO">
        SELECT *
        FROM us
       	WHERE user_name = #{userName}
    </select>
	
	

